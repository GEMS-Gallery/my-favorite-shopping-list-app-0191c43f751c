type Time = int;
type ShoppingItem = 
 record {
   completed: bool;
   createdAt: Time;
   description: text;
   id: nat;
   "text": text;
 };
type Result = 
 variant {
   err: text;
   ok;
 };
service : {
  addItem: (text, text) -> (nat);
  deleteItem: (nat) -> (Result);
  editItem: (nat, text, text) -> (Result);
  getItems: () -> (vec ShoppingItem) query;
  markItemCompleted: (nat) -> (Result);
}
